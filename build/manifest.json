{
  "name": "HWiNFO Summarize",
  "description": "Summarize the .HTM report files from HWiNFO",
  "version": "0.1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "28",
  "offline_enabled": true,
  "icons": {
    "16": "app-logo.png",
    "128": "app-logo.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    },
    "storage",
    "https://%20//fonts.googleapis.com"
  ]
}